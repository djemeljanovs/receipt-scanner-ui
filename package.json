{
  "name": "Resin",
  "scripts": {
    "test": "TZ=UTC jest --no-cache --coverage",
    "clean": "trash frontend/dist .awcache coverage",
    "start": "yarn clean && yarn dll && yarn dev",
    "dev": "yarn clean && yarn dll && cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.dev.babel.js",
    "build": "yarn clean && yarn dll && cross-env NODE_ENV=production webpack --config webpack.config.prod.babel.js",
    "dll": "cross-env NODE_ENV=production webpack --config webpack.config.dll.babel.js",
    "storybook": "start-storybook -p 6006 -s ./stories",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.5",
