    "webpack-hot-middleware": "^2.24.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.babel.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      ".d.ts",
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "globals": {
      "TZ": "UTC",
      "ts-jest": {
        "diagnostics": true
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/stories/"
    ]
  }
}
