    "@types/fetch-mock": "^6.0.4",
    "@types/immutability-helper": "^2.6.3",
    "@types/jest": "^23.3.2",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.9.4",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.7",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.0",
